#. type: Plain text
#: upstream/_layouts/post.html:44
#, no-wrap
msgid ""
"<div class=\"post-page grid-wrapper\">\n"
"  <div class=\"width-8-12 width-12-12-m doc-content\">\n"
"    <div class=\"grid-wrapper\">\n"
"      <div class=\"width-12-12\">\n"
"        <p>\n"
"          <a href=\"{{site.baseurl}}/blog\"><i class=\"fas "
"fa-angle-left\"></i> Back to all posts</a>\n"
"        </p>\n"
"      </div>\n"
"      <div class=\"width-12-12\">\n"
"        <div class=\"post-date\">\n"
"          {{ page.date | date: '%B %d, %Y' }} \n"
"          {% if page.tags %}\n"
"            <span class=\"tags\">{% for tag in page.tags %}<a "
"href=\"{{site.baseurl}}/blog/tag/{{tag}}\">#{{ tag}}</a>{% endfor "
"%}</span>\n"
"          {% endif %}\n"
"        </div>\n"
"        <h1 class=\"post-title\">{{ page.title }}</h1>\n"
"        <div class=\"grid-wrapper\">\n"
"          <div class=\"width-8-12 width-12-12-m byline-wrapper\">\n"
"            {% assign author = site.data.authors[page.author] %}\n"
"            {% if author.emailhash %}\n"
"              <img class=\"headshot\" "
"src=\"https://www.gravatar.com/avatar/{{ author.emailhash }}\">\n"
"            {% endif %}\n"
"            <p class=\"byline\">By {{ author.name }}</p>\n"
"          </div>\n"
"          <div class=\"width-12-12\">\n"
"              {{ content }}\n"
"              {% if page.bibquery %}\n"
"                {% assign publications = site.data.publications %}\n"
"                {% for query in page.bibquery %}\n"
"                  {% assign publications = publications | where_exp: "
"\"pub\", query %}\n"
"                {% endfor %}\n"
"                {% include publication-entries.html %}\n"
"              {% endif %}\n"
"          </div>\n"
"          <div class=\"width-12-12\">{% include share-page.html url=page.url "
"title=page.title %}</div>\n"
"        </div>\n"
"      </div>\n"
"    </div>\n"
"  </div>\n"
"</div>\n"
msgstr ""
