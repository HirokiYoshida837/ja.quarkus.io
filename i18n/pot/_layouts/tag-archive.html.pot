#. type: Plain text
#: upstream/_layouts/tag-archive.html:13
#, no-wrap
msgid ""
"<div class=\"grid-wrapper blog-page\">\n"
"  <div class=\"grid__item width-10-12\">\n"
"    <h1 class=\"text-caps\">Blog</h1>\n"
"  </div>\n"
"  <div class=\"grid__item width-2-12 align-self-center\">\n"
"    <a href=\"{{site.baseurl}}/feed.xml\"><i class=\"fas "
"fa-rss-square\"></i></a>\n"
"  </div>\n"
msgstr ""

#. type: Plain text
#: upstream/_layouts/tag-archive.html:59
#, no-wrap
msgid ""
"  <div class=\"grid__item width-9-12 width-12-12-m\">\n"
"    <h2 class=\"title\">Tagged posts: '{{ page.title }}'</h2>\n"
"    {% for post in page.posts %}\n"
"      {% assign author = site.data.authors[post.author] %}\n"
"      <div class=\"blog-list-item grid-wrapper\">\n"
"        <div class=\"grid__item width-12-12\">\n"
"          <div class=\"post-title\">\n"
"            <a href=\"{{site.baseurl}}{{ post.url }}\">{{ post.title "
"}}</a>\n"
"          </div>\n"
"          <div class=\"post-date small\">\n"
"            {{ post.date | date: '%B %d, %Y' }} \n"
"            {% if post.tags %}\n"
"              <span class=\"tags\">tags:{% for tag in post.tags %}<a "
"href=\"{{site.baseurl}}/blog/tag/{{tag}}\">#{{ tag}}</a>{% endfor "
"%}</span>\n"
"            {% endif %}\n"
"          </div>\n"
"        </div>\n"
"        <div class=\"grid__item width-8-12 width-12-12-m byline-wrapper\">\n"
"          {% if author.emailhash %}\n"
"            <img class=\"headshot\" src=\"https://www.gravatar.com/avatar/{{ "
"author.emailhash }}\">\n"
"          {% endif %}\n"
"          <p class=\"byline\"><span>By</span> {{ author.name }}<br/></p>\n"
"        </div>\n"
"        <div class=\"grid__item width-12-12\">\n"
"          {% if post.synopsis %}\n"
"            <p>{{ post.synopsis | strip_html }}</p>\n"
"          {% else %}\n"
"            <p>{{ post.content | strip_html | truncatewords: 75 }}</p>\n"
"          {% endif %}\n"
"        </div>\n"
"        <div class=\"grid__item width-8-12 read-more small\"><a "
"href=\"{{site.baseurl}}{{ post.url }}\">Read More &rsaquo;</a></div>\n"
"        <div class=\"grid__item width-4-12 width-12-12-m share-post\">{% "
"include share-page.html title=post.title url=post.url %}</div>\n"
"      </div>\n"
"    {% endfor %}\n"
"  </div>\n"
"  <div class=\"grid__item width-1-12 width-12-12-m\"></div>\n"
"  <div class=\"grid__item width-2-12 width-12-12-m\">\n"
"    <h3 class=\"tags-label\">Tags</h3>\n"
"    {% assign tag_words = site.tags | sort %}\n"
"    {% for stats in tag_words %}\n"
"    {% assign tag = stats | first %}\n"
"    {% assign posts = stats | last %}\n"
"    <a href=\"{{site.baseurl}}/blog/tag/{{tag}}\">{{ tag }}</a></br>\n"
"    {% endfor %}\n"
"  </div>\n"
"</div>\n"
msgstr ""
