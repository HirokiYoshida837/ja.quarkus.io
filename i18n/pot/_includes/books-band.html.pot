msgid ""
"<div class=\"component-wrapper books-band\">\n"
"  <div class=\"width-12-12\">\n"
"    <h3 class=\"mt-0\">Books</h3>\n"
"  </div>\n"
"  {% for item in site.data.books %}\n"
"    <div class=\"width-6-12 book\">\n"
"      <div class=\"grid-wrapper\">\n"
"        <div class=\"img-wrapper\">\n"
"          <a href=\"{{ item.link }}\" target=\"_blank\">\n"
"            <img src=\"{{site.baseurl}}/assets/images/books/{{ "
"item.thumbnail }}\">\n"
"          </a>\n"
"        </div>\n"
"        <div class=\"info-wrapper\">\n"
"          <h4 class=\"mt-0\"><a href=\"{{ item.link }}\" "
"target=\"_blank\">{{ item.title }}</a></h4>\n"
"          <p>{{ item.description }}</p>\n"
"          <small>\n"
"            Publish Date: {{ item.publish-date }}<br/>\n"
"            Author(s): {{ item.authors }}\n"
"          </small>\n"
"        </div>\n"
"      </div>\n"
"    </div>\n"
"  {% endfor %}\n"
"</div>\n"
msgstr ""
