#. type: Plain text
#: upstream/_includes/homepage-imperative_and_reactive-band.html:17
#, no-wrap
msgid ""
"<div class=\"full-width-bg component homepage-content-band "
"alt-background\">\n"
"  <div class=\"grid-wrapper\">\n"
"    <div class=\"width-12-12 block-content\">\n"
"      <h2>Unifies Imperative and Reactive</h2>\n"
"    </div>\n"
"    <div class=\"width-12-12 width-12-12-m\">\n"
"      <p>Combine both the familiar imperative code and the non-blocking "
"reactive style when developing applications.\n"
"      <a href=\"/vision/continuum\">Learn more</a>.</p>\n"
"    </div>\n"
"    <div class=\"width-12-12 width-12-12-m\">\n"
"      <div class=\"grid-wrapper\">\n"
"        <div class=\"width-6-12 code-item\">\n"
"          <div class=\"code-label\">Imperative</div>\n"
"          <div class=\"code-wrapper\">\n"
"            <pre class=\"highlight\"><code>@Inject\n"
"SayService say;\n"
msgstr ""

#. type: Plain text
#: upstream/_includes/homepage-imperative_and_reactive-band.html:30
#, no-wrap
msgid ""
"@GET\n"
"@Produces(MediaType.TEXT_PLAIN)\n"
"public String hello() {\n"
"    return say.hello();\n"
"}</code></pre>\n"
"          </div>\n"
"        </div>\n"
"        <div class=\"width-6-12 code-item\">\n"
"          <div class=\"code-label\">Reactive</div>\n"
"          <div class=\"code-wrapper\">\n"
"            <pre class=\"highlight\"><code>@Inject @Channel(\"kafka\")\n"
"Publisher&lt;String&gt; reactiveSay;\n"
msgstr ""

#. type: Plain text
#: upstream/_includes/homepage-imperative_and_reactive-band.html:41
#, no-wrap
msgid ""
"@GET\n"
"@Produces(MediaType.SERVER_SENT_EVENTS)\n"
"public Publisher&lt;String&gt; stream() {\n"
"    return reactiveSay;\n"
"}</code></pre>\n"
"          </div>\n"
"        </div>\n"
"      </div>\n"
"    </div>\n"
"  </div>\n"
"</div>\n"
msgstr ""
